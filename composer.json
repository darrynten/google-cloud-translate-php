{
  "name": "darrynten/google-cloud-translate-php",
  "description": "PHP Google Cloud Translate Client with Extras",
  "type": "library",
  "require": {
    "php": "^7.0",
    "google/cloud": "0.*.*",
    "darrynten/any-cache": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "mockery/mockery": "dev-master"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "DarrynTen\\GoogleCloudTranslatePhp\\": "src/"
    },
    "exclude-from-classmap": ["/Tests/"]
  }
}
